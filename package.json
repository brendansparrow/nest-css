{
  "name": "nest-ui-system",
  "version": "1.0.0",
  "description": "A lightweight front-end CSS framework and methodology for building web interfaces.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "run-p watch:kss serve:kss",
    "kss": "kss --config kss-config.json",
    "watch:kss": "onchange 'sass/**/*.sass' -- npm run kss",
    "serve:kss": "http-server ./styleguide -o",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brendansparrow/nest-ui-system.git"
  },
  "author": "Brendan Sparrow",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brendansparrow/nest-ui-system/issues"
  },
  "homepage": "https://github.com/brendansparrow/nest-ui-system#readme",
  "devDependencies": {
    "http-server": "^0.11.1",
    "kss": "^3.0.0-beta.25",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0"
  }
}
